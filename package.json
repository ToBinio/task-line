    "reka-ui": "^2.2.0",
    "tailwindcss": "^4.1.3",
    "uuid": "^11.1.0",
    "vaul-vue": "^0.4.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@iconify-json/material-symbols": "^1.2.17",
    "@nuxt/eslint": "^1.3.0",
    "@pinia/nuxt": "^0.10.1",
    "@vueuse/nuxt": "^13.0.0",
    "eslint": "^9.24.0",
